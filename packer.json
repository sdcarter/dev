{
    "builders": [
      {
        "commit": true,
        "image": "debian:latest",
        "type": "docker"
      }
    ],
    "post-processors": [
      [
        {
          "repository": "sdcarter/dev-container",
          "tags": [
            "latest"
          ],
          "type": "docker-tag"
        },
        {
          "type": "docker-push"
        }
      ]
    ],
    "provisioners": [
      {
        "script": "install.sh",
        "type": "shell"
      },
      {
        "playbook_file": "./playbook.yml",
        "type": "ansible"
      }
    ]
  }